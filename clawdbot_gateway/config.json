{
  "name": "OpenClaw Gateway",
  "version": "0.2.17",
  "slug": "clawdbot_gateway",
  "description": "OpenClaw Gateway (SSH-tunneled, HA add-on)",
  "url": "https://github.com/openclaw/openclaw",
  "documentation": "https://docs.openclaw.ai",
  "arch": ["aarch64", "armv7", "amd64"],
  "startup": "services",
  "boot": "auto",
  "host_network": true,
  "hassio_api": false,
  "homeassistant_api": false,
  "map": ["config:rw"],
  "environment": {
    "HOME": "/config/openclaw",
    "GH_CONFIG_DIR": "/config/openclaw/.config/gh"
  },
  "options": {
    "port": 18789,
    "verbose": false,
    "repo_url": "https://github.com/openclaw/openclaw.git",
    "branch": "",
    "github_token": "",
    "ssh_port": 2222,
    "ssh_authorized_keys": ""
  },
  "schema": {
    "port": "int(1,65535)",
    "verbose": "bool",
    "repo_url": "str",
    "branch": "str?",
    "github_token": "password?",
    "ssh_port": "int(1,65535)",
    "ssh_authorized_keys": "str?"
  }
}
